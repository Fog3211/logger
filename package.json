{
  "name": "@airmus/logger",
  "version": "0.2.1",
  "description": "A javaScript library for easy control of logging",
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "install_all": "npm install && cd demo && npm install",
    "start": "cd demo && npm run dev",
    "build_test": "rollup -c -w",
    "build": "rollup -c",
    "release": "bash publish.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --parser=typescript --write"
    ]
  },
  "files": [
    "dist",
    "yarn.lock",
    "README.md"
  ],
  "keywords": [
    "logger",
    "log",
    "console"
  ],
  "author": "Fog3211",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Airmus/logger.git"
  },
  "bugs": {
    "url": "https://github.com/Airmus/logger/issues"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@umijs/fabric": "^2.8.1",
    "del": "^6.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "rollup": "^2.60.2",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "0.31.1",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
